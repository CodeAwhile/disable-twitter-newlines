{
   "name": "Disable Twitter Newlines",
   "version": "1.3",
   "manifest_version": 2,
   
   "description": "Simple Chrome extension that disables newlines in tweets.",
   "icons": {
      "128": "disable-twitter-newline-large.png",
      "48": "disable-twitter-newline-small.png"
   },
   
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "*://twitter.com/*" ]
   } ]
}